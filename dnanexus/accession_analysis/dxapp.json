{
  "name": "accession_analysis",
  "title": "Accession analysis",
  "summary": "accession_analysis",
  "dxapi": "1.0.0",
  "version": "0.0.1",
  "categories": [
    "ENCODE Analysis Pipeline"
  ],
  "inputSpec": [
    {
      "name": "analysis_ids",
      "class": "array:string",
      "optional": true
    },
    {
      "name": "infile",
      "class": "file",
      "optional": true
    },
    {
      "name": "outfn",
      "class": "string",
      "optional": true,
      "default": "accession_analysis.csv"
    },
    {
      "name": "debug",
      "class": "boolean",
      "optional": true,
      "default": false
    },
    {
      "name": "project",
      "class": "string",
      "optional": true
    },
    {
      "name": "pipeline",
      "class": "string",
      "optional": true
    },
    {
      "name": "key",
      "class": "string",
      "optional": true
    },
    {
      "name": "keyfile",
      "class": "string",
      "optional": true,
      "default": "keypairs.json"
    },
    {
      "name": "dryrun",
      "class": "boolean",
      "optional": true,
      "default": false
    },
    {
      "name": "force_patch",
      "class": "boolean",
      "optional": true,
      "default": false
    },
    {
      "name": "force_upload",
      "class": "boolean",
      "optional": true,
      "default": false
    },
    {
      "name": "fqcheck",
      "class": "boolean",
      "optional": true,
      "default": true
    },
    {
      "name": "accession_raw",
      "class": "boolean",
      "optional": true,
      "default": true
    },
    {
      "name": "signal_only",
      "class": "boolean",
      "optional": true,
      "default": false
    },
    {
      "name": "skip_control",
      "class": "boolean",
      "optional": true,
      "default": false
    }
  ],
  "outputSpec": [
    {
      "name": "outfile",
      "class": "file"
    }
  ],
  "runSpec": {
    "interpreter": "python2.7",
    "file": "src/accession_analysis.py",
    "execDepends": [
      {"name": "awscli", "package_manager": "pip"}
    ]

  },
  "access": {
    "network": ["*"],
    "project": "CONTRIBUTE",
    "allProjects": "CONTRIBUTE"
  },
  "authorizedUsers": []
}
